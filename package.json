{
  "name": "listing-analysis-automation",
  "version": "1.0.0",
  "description": "Automated Amazon listing analysis using Firecrawl, AWS Rekognition, and ChatGPT with LangGraph workflow orchestration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "schedule": "node dist/scheduler.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:manual": "tsx src/test.ts"
  },
  "keywords": [
    "amazon",
    "firecrawl",
    "aws-rekognition",
    "chatgpt",
    "langgraph",
    "automation",
    "listing-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.919.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "@matthewkerns/software-development-best-practices-guide": "^1.3.7",
    "dotenv": "^16.4.7",
    "googleapis": "^144.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.16",
    "@vitest/coverage-v8": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.0.0"
  }
}
